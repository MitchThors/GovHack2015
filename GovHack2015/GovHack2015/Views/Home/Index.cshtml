@using GovHack2015.Logic
@using GovHack2015.Models
@model GovHack2015.Models.Article
<table class="form-control table-striped">

    @{
        var ga = new GetArticles();
        var articleList = ga.PopulateArticles();
    }

    @foreach (var record in articleList)
    {
        <tr>




            <td>@record.Title</td>


        </tr>


    }
</table>